#!/usr/bin/env node

const program = require('commander')
const package = require('../package.json')
const init = require('../init')

// now, no options
program
  .usage('[options] <ProjectName>')
  .version(package.version)
  .option('init', 'generate initial project')
  .parse(process.argv)

if (program.init) {
  init(program.args[0])
  return
}

console.log('error')
